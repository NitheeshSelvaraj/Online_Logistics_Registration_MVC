<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href="~/Content/Site.css" />
</head>
<body class="background">
    <div class="Head">
        <h1>Online Logistics Registration</h1>
        </div>
    <div class="align">
        <div class="empty"></div>
        <div class="register">
            @Html.ActionLink("Register", "Register", "Online_Logistics_Registration_User", new { @class="link" })
        </div>
        <div class="login">
            @Html.ActionLink("Login", "Login", "Online_Logistics_Registration_User", new { @class = "link" })
        </div>
        <div class="home">
            @Html.ActionLink("Home", "Index", "Online_Logistics_Registration_User", new { @class = "link" })
        </div>       
    </div>
    <div>
        @RenderBody()
    </div>
</body>
</html>
